<!-- {company, student_list} -->
<div  id="sheduled-interviews-table<%= company._id %>">
    <table class="table table-striped">
        <thead>
            <tr>
                <th scope="col">S.No</th>
                <th scope="col">Student Name</th>
                <th scope="col">Profile</th>
                <th scope="col">Result</th>
            </tr>
        </thead>
        <tbody>
            <% count=1 %>
            <% for(k of company.students){ %>
                <tr>
                    <th scope="row"><%= count %></th>
                    <td><%= k.studentId.name %></td>
                    <td><%= k.profile %></td>
                    <td><%= k.result %></td>
                    <td><%- include('update_sheduled_interview', {k, company, student_list}); %> </td>
                    <td><%- include('delete_sheduled_interview', {k, company}); %> </td>
                </tr>
            <% count = count+1 %>
            <% } %>
        </tbody>
    </table>
</div>
